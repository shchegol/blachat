{
  "compilerOptions": {
    "module": "es6",
    "esModuleInterop": true,
    "target": "es2017",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "strictNullChecks": true
  }
}