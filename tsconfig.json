{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "static/build",
    "module": "es6",
    "esModuleInterop": true,
    "target": "es2016",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "strictNullChecks": true
  }
}